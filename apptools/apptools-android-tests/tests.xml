        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="AppTools_Help" purpose="Android - Validate if all usages work well as description">
        <description>
          <test_script_entry>/opt/apptools-android-tests/apptools/AppTools_Help.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
