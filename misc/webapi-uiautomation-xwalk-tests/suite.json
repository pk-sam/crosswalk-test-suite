{
    "pkg-blacklist": [
        "pack.py",
        "COPYING",
        "testcase.xsl",
        "testresult.xsl",
        "tests.css",
        "icon.png",
        "suite.json",
        "inst.*.py",
        "manifest.json"
    ],
    "pkg-list": {
        "apk": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "bddrunner": "bddrunner",
                "html5-extra-app/testscripts": "html5-extra-app/testscripts",
                "inst.apk.py": "inst.py",
                "tests.xml": "tests.xml",
                "w3c-webstorage-app/testscripts": "w3c-webstorage-app/testscripts"
            },
            "pkg-app": {
                "apk-common-opts": "--enable-remote-debugging",
                "blacklist": [
                    "html5-extra-app",
                    "w3c-webstorage-app"
                ]
            },
            "subapp-list": {
                "html5-extra-app": {
                    "apk-common-opts": "--enable-remote-debugging",
                    "app-name": "html5-extra-app",
                    "install-path": "apps"
                },
                "w3c-webstorage-app": {
                    "apk-common-opts": "--enable-remote-debugging",
                    "app-name": "w3c-webstorage-app",
                    "install-path": "apps"
                }
            }
        }
    },
    "pkg-name": "webapi-uiautomation-xwalk-tests"
}
