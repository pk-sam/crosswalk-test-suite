<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Lowresource" name="stability-lowresource-tizen-tests">
    <set name="lowresource" type="script">
      <testcase purpose="Validate play video under low disk" type="Functional" status="approved" component="Stability/Lowresource" execution_type="auto" priority="P1" id="Play_OnlineVideo_UnderLowDisk">
        <description>
          <pre_condition>
	    1. Make sure the device under low resource:
	       dev/root and dev used 100%, dev/shm used > 60%
          </pre_condition>
          <test_script_entry test_script_expected_result="0" timeout="14401">app_user@TESTER-HOME-DIR/content/tct/opt/stability-lowresource-tizen-tests/lowresource/Play_OnlineVideo_UnderLowDisk.sh</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate install app under low disk" type="Functional" status="approved" component="Stability/Lowresource" execution_type="auto" priority="P1" id="Install_Webapp_UnderLowDisk">
        <description>
          <pre_condition>
	    1. Make sure the device under low resource:
	       dev/root and dev used 100%, dev/shm used > 60%
          </pre_condition>
          <test_script_entry test_script_expected_result="0" timeout="14401">app_user@TESTER-HOME-DIR/content/tct/opt/stability-lowresource-tizen-tests/lowresource/Install_Webapp_UnderLowDisk.sh</test_script_entry>
          </description>
          </testcase>        
    </set>
  </suite>
</test_definition>

