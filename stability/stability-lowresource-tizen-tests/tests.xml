<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Lowresource" name="stability-lowresource-tizen-tests">
    <set name="lowresource" type="script">
      <testcase component="Stability/Lowresource" execution_type="auto" id="Play_OnlineVideo_UnderLowDisk" purpose="Validate play video under low disk">
        <description>
          <pre_condition>
            1. Make sure the device under low resource:
               dev/root and dev used 100%, dev/shm used > 60%
          </pre_condition>
          <test_script_entry test_script_expected_result="0" timeout="150">app_user@TESTER-HOME-DIR/content/tct/opt/stability-lowresource-tizen-tests/lowresource/Play_OnlineVideo_UnderLowDisk.sh</test_script_entry>
        </description>
      </testcase>
      <testcase component="Stability/Lowresource" execution_type="auto" id="Install_Webapp_UnderLowDisk" purpose="Validate install app under low disk">
        <description>
          <pre_condition>
            1. Make sure the device under low resource:
               dev/root and dev used 100%, dev/shm used > 60%
          </pre_condition>
          <test_script_entry test_script_expected_result="0" timeout="150">app_user@TESTER-HOME-DIR/content/tct/opt/stability-lowresource-tizen-tests/lowresource/Install_Webapp_UnderLowDisk.sh</test_script_entry>
          </description>
          </testcase>        
    </set>
  </suite>
</test_definition>
