{
    "pkg-blacklist": [
        "config.xml",
        "pack.py",
        "testcase.xsl",
        "testresult.xsl",
        "tests.css",
        "icon.png",
        "manifest.json",
        "suite.json",
        "inst.*"
    ],
    "pkg-list": {
        "apk,cordova": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "bddrunner": "bddrunner",
                "html5-extra-app/testscripts": "html5-extra-app/testscripts",
