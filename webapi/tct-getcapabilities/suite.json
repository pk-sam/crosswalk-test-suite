{
    "pkg-blacklist": [
        "config.xml",
        "pack.py",
        "icon.png",
        "manifest.json",
        "suite.json",
        "inst.*"
    ],
    "pkg-list": {
        "apk,cordova": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "inst.apk.py": "inst.py"
            },
            "pkg-app": {}
        },
        "apk-aio, cordova-aio": {
            "blacklist": []
        },
        "wgt": {
            "blacklist": [
                "index.html",
                "js"
            ],
            "copylist": {
                "inst.wgt.py": "inst.py"
            },
            "subapp-list": {
                ".": {
                    "blacklist": [
                        "suite.json",
                        "manifest.json",
                        "inst.*"
                    ],
                    "app-dir": ".",
                    "sign-flag": "true",
                    "app-name": "tct-getcapabilities"
                }
            }
        },
        "xpk": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "inst.xpk.py": "inst.py"
            },
            "pkg-app": {
                "blacklist": []
            }
        }
    },
    "pkg-name": "tct-getcapabilities"
}
