<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Crosswalk WRT" name="wrt-sharedmodemanu-android-tests">
    <set name="sharedmodemanu" type="js">
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_Library_Icon_Check" purpose="Validate if Crosswalk icon display normally">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_Library_Icon_Check.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_Library_Name_Check" purpose="Validate the installer name is about with Crosswalk">
        <description>
          <pre_condition>
            1.Make sure the Crosswalk runtime 'XwalkRuntimeLib.apk'  already exists in the Android OS
          </pre_condition>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_Library_Name_Check.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_embeded" purpose="Validate descriptive error message prompt if install arm app in x86 platform">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_embeded.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_embeded" purpose="Validate descriptive error message prompt if install x86 app in arm platform">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_embeded.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_arm_shared" purpose="Validate descriptive error message prompt if install shared arm app in arm platform">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_arm_shared.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_shared_x86_crosswalk" purpose="Validate descriptive error message prompt if install shared arm app in arm platform with x86 RuntimeLib">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_shared_x86_crosswalk.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_x86_shared" purpose="Validate descriptive error message prompt if install shared x86 app in x86 platform">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_x86_shared.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_shared_arm_crosswalk" purpose="Validate descriptive error message prompt if install shared x86 app in x86 platform with arm RuntimeLib">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_shared_arm_crosswalk.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_arm_x86_shared" purpose="Validate the app built with shared mode can run on both of the IA and ARM devices">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_arm_x86_shared.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_shared_download_break" purpose="Validate the app built with shared mode can run on both of the IA and ARM devices">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_shared_download_break.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_x86_shared_arm_2" purpose="Validate descriptive error message prompt if install shared x86 app in arm platform with arm RuntimeLib">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_x86_shared_arm_2.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Shared Mode" execution_type="manual" id="Crosswalk_arch_mismatch_arm_shared_x86_2" purpose="Validate descriptive error message prompt if install shared arm app in x86 platform with x86 RuntimeLib">
        <description>
          <test_script_entry>/opt/wrt-sharedmodemanu-android-tests/sharedmodemanu/Crosswalk_arch_mismatch_arm_shared_x86_2.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
